{
  "functions": {
    "app/api/register.js": {
      "runtime": "nodejs18.x"
    },
    "app/api/registrations.js": {
      "runtime": "nodejs18.x"
    },
    "app/api/attendees/[attendeeId].js": {
      "runtime": "nodejs18.x"
    },
    "app/api/ticket/[ticketId].js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "JWT_SECRET": "@jwt_secret",
    "NODE_ENV": "production"
  },
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/register",
      "destination": "/app/api/register.js"
    },
    {
      "source": "/api/registrations",
      "destination": "/app/api/registrations.js"
    },
    {
      "source": "/api/attendees/:attendeeId",
      "destination": "/app/api/attendees/[attendeeId].js"
    },
    {
      "source": "/api/ticket/:ticketId",
      "destination": "/app/api/ticket/[ticketId].js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}