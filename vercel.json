{
  "functions": {
    "api/register.ts": {
      "runtime": "nodejs20.x"
    },
    "api/registrations.ts": {
      "runtime": "nodejs20.x"
    },
    "api/attendees/[attendeeId].ts": {
      "runtime": "nodejs20.x"
    },
    "api/ticket/[ticketId].ts": {
      "runtime": "nodejs20.x"
    }
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "rewrites": [
    {
      "source": "/api/register",
      "destination": "/api/register"
    },
    {
      "source": "/api/registrations",
      "destination": "/api/registrations"
    },
    {
      "source": "/api/attendees/:attendeeId",
      "destination": "/api/attendees/[attendeeId]"
    },
    {
      "source": "/api/ticket/:ticketId",
      "destination": "/api/ticket/[ticketId]"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}